{
  "name": "food-truck",
  "version": "0.0.1",
  "description": "food truck app",
  "main": "index.js",
  "scripts": {
    "build:client": "npx webpack --config config/webpack.client.config.js",
    "build:server": "npx webpack --config config/webpack.server.config.js",
    "build": "npm run build:client && npm run build:server",
    "start-client": "webpack serve --config config/webpack.client.config.js",
    "start-server": "node src/server/index.js",
    "start": "concurrently \"npm run start-client\" \"npm run start-server\""
  },
